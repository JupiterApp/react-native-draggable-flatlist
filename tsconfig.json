{
	"compilerOptions": {
		"module":"es2015",
		"target": "es2015",
		"jsx": "react",
		"rootDir": "src",
		"outDir": "built",
		"allowSyntheticDefaultImports": true,
		"noImplicitAny": true,
		"sourceMap": true,
		"experimentalDecorators": true,
		"preserveConstEnums": true,
		"allowJs": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"noImplicitReturns": true,
		"skipLibCheck": true,
		"moduleResolution":"Node"
	},
	"exclude": ["node_modules", "built"],
	"include": ["./src"]
}